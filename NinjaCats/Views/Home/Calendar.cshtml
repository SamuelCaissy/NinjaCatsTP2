
@{
    ViewData["Title"] = "Calendar";
}
@model IEnumerable<dynamic>
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Image</th>
            <th>Nom</th>
            <th>Addresse</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>

                @if (item.Type == "membre")
                {
                    <td> <img src="/images/imgHolder.png" class="img-fluid" alt="Membre Image" /></td>
                }
                else
                {
                    <td>  <img src="/images/imgHolder2.png" class="img-fluid" alt="Other Type Image" /></td>
                }
                
                <td>@item.Name</td>
                <td>@item.Address</td>
                <td>@item.Type</td>
            </tr>
        }
    </tbody>
</table>
